@{
    Layout = null;
}

    
[ @{
    int counter = ViewBag.mmap.Count;
    @foreach (KeyValuePair<string, int> item in ViewBag.mmap)
        {
            @if (counter > 1)
            {
                <text>{"GpioStatus":@item.Value, "Machineid":"@item.Key"}, </text>
            }
            else
            {
                <text>{"GpioStatus":@item.Value, "Machineid":"@item.Key"} </text>
            }
            counter = counter - 1;
    }
} ]
